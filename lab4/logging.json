{
  "version": 1,
    "formatters": {
      "simple": {
        "format": "%(asctime)s: %(message)s",
        "datefmt": "%I:%M:%S %d.%m.%Y"
      }
    },
    "handlers": {
      "console": {
        "class": "logging.StreamHandler",
        "level": "DEBUG",
        "formatter": "simple",
        "stream": "ext://sys.stdout"
      }
    },
    "root": {
      "level": "INFO",
      "handlers": [
        "console"
      ]
    }
  }